<div class="wrapper">
  <div class="container">
  <h1>schedule_edit</h1>
    <%= form_with model: @schedule, url: schedule_path do |form| %>
      <%= form.text_field :name, class:"form-group"%>
      <%= form.text_field :password, class:"form-group"%>
    
    <h1>概要</h1>
      <%= form.text_field :title, class:"form-group"%>
      <%= form.text_area :content, class:"form-group"%>
      <%= form.submit "編集内容を保存", class:"form-group"%>
    <% end %>

    <h1>プラン</h1>
    <%= form_with url:"#" do |form| %>
      <%= form.date_field :hoge, placeholder: "", class:"form-group"%>
      <%= form.time_field :hoge, placeholder: "", class:"form-group"%>
      <%= form.text_field :hoge, placeholder: "タイトル", class:"form-group"%>
      <%= form.text_area :hoge, placeholder: "説明", class:"form-group"%>
      <%= form.submit "プラン追加", class:"form-group"%>
    <% end %>
  </div>
</div>